{
  "name": "font-decision-engine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo 'No tests defined yet' && exit 0",
    "pages:build": "npx @cloudflare/next-on-pages",
    "crawl:preview": "tsx scripts/crawlers/noonnu-crawler.ts --preview",
    "crawl:noonnu": "tsx scripts/crawlers/noonnu-crawler.ts --limit=50",
    "seed:noonnu": "tsx scripts/seed/index.ts --source=noonnu --skip-existing",
    "seed:mock": "tsx scripts/seed/index.ts --source=mock --skip-existing",
    "seed:dry-run": "tsx scripts/seed/index.ts --source=noonnu --dry-run",
    "tags:generate": "tsx scripts/ai/generate-emotion-tags.ts --limit=20",
    "tags:dry-run": "tsx scripts/ai/generate-emotion-tags.ts --limit=5 --dry-run",
    "seed:experts": "tsx scripts/seed/experts.ts"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "15.5.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "15.5.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
